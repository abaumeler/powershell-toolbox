<Settings>
	<General>
		<LogPath>C:\Path\to\Logs</LogPath>
		<RoboArgs>/r:5 /v /fp /ts /e /w:1 /NP</RoboArgs>
		<PrefixRemoveRegex>([\d]{2}.[\d]{2}.[\d]{2}_)|([\d]{2}.[\d]{2}.[\d]{2}.[\d]{2})|(_{0,1}[\d]{1,3}.[\d]{2}.[\d]{2}.[\d]{2})</PrefixRemoveRegex>
		<IgnoreFoldersList>Archive;Archiv</IgnoreFoldersList>
	</General>
	<JobList>
		<Job>
			<SourcePath>\\path\to\source\folder</SourcePath>
			<DestinationPath>G:\path\to\destination\folder</DestinationPath>
			<CleanPrefix>1</CleanPrefix>
			<UseIgnoreFolders>0</UseIgnoreFolders>
			<Name>Job_Name</Name>
		</Job>
		<Job>
			<SourcePath>\\path\to\source\folder</SourcePath>
			<DestinationPath>G:\path\to\destination\folder</DestinationPath>
			<CleanPrefix>0</CleanPrefix>
			<UseIgnoreFolders>1</UseIgnoreFolders>
			<Name>SecondJob_Name</Name>
		</Job>
	</JobList>
</Settings>